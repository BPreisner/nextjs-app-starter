{
   "name": "nextjs-app-starter",
   "version": "1.0.0",
   "description": "NextJsStarterKit",
   "main": "index.js",
   "scripts": {
      "dev": "nodemon",
      "build": "next build src/routes && tsc --project tsconfig.server.json && yarn eslintCheck",
      "start": "cross-env NODE_ENV=production node dist/server/index.js",
      "test": "jest",
      "tsc": "tsc",
      "eslintCheckSrc": "eslint src/**",
      "eslintCheckServer": "eslint server/**",
      "eslint": "yarn eslintCheckSrc && yarn eslintCheckServer"
   },
   "engines": {
      "node": ">=10.0.0",
      "yarn": ">=1.16.0"
   },
   "repository": {
      "type": "git",
      "url": "https://google.com"
   },
   "author": "BPreisner",
   "license": "MIT",
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "dependencies": {
      "@zeit/next-typescript": "^1.1.1",
      "axios": "^0.19.0",
      "lodash": "^4.17.11",
      "next": "^8.1.0",
      "next-redux-wrapper": "^3.0.0-alpha.2",
      "react": "^16.8.6",
      "react-dom": "^16.8.6",
      "react-redux": "^7.0.3",
      "redux": "^4.0.1",
      "styled-components": "^4.2.0"
   },
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/enzyme": "^3.9.3",
      "@types/jest": "^24.0.13",
      "@types/lodash": "^4.14.133",
      "@types/next": "^8.0.5",
      "@types/next-redux-wrapper": "^2.0.2",
      "@types/node": "^12.0.4",
      "@types/react": "^16.8.19",
      "@types/react-dom": "^16.8.4",
      "@types/react-redux": "^7.0.9",
      "@types/redux": "^3.6.0",
      "@types/styled-components": "^4.1.15",
      "@typescript-eslint/eslint-plugin": "^1.9.0",
      "@typescript-eslint/parser": "^1.9.0",
      "babel-core": "^6.26.3",
      "babel-jest": "^24.8.0",
      "babel-plugin-styled-components": "^1.10.0",
      "cross-env": "^5.2.0",
